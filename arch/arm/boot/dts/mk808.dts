/dts-v1/;
#include "rk3066a.dtsi"

/ {
	chosen {
		bootargs = "debug console=ttyS0,115200 root=/dev/mmcblk0p1 rootwait init=/bin/bash";
	};

	memory {
		reg = <0x60000000 0x20000000>;
	};

	soc {

		dwmmc@10214000 {
			num-slots = <1>;
			status = "okay";
			pinctrl-names = "default";
			pinctrl-0 = <&sd0_clk &sd0_cmd &sd0_bus4 &sd0_cd>;
		};

		dwmmc@10218000 {
			num-slots = <1>;
			status = "okay";
			pinctrl-names = "default";
			pinctrl-0 = <&sd1_clk &sd1_cmd &sd1_bus4 &sd1_cd>;
		};

		uart2: serial@20064000 {
			status = "okay";
		};

	};
};
